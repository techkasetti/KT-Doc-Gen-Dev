        <name>Failed_Quantity</name>
        <dataType>Number</dataType>
        <expression>VALUE({!GetLabelLineDetail.Order_Quantity__c}) - VALUE({!GetLabelLineDetail.Print_Quantity__c})</expression>
        <scale>2</scale>
    </formulas>
