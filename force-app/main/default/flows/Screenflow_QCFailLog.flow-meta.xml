    <formulas>
        <description>increment of 1 to  Total_Reprint_Version__c in parent line</description>
        <name>Add_To_Reprint</name>
        <dataType>Number</dataType>
        <expression>IF(
    ISBLANK({!GetLabelLineDetail.Parent_Line__c}),
    0,  
    {!GetLabelLineDetail.Parent_Line__r.Total_Reprint_Version__c} + 1  
)</expression>
        <scale>2</scale>
    </formulas>
