        <dataType>Number</dataType>
        <expression>{!PrintQtyFormula1}</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <description>Comma removal from Print QTY</description>
        <name>PrintQtyFormula</name>
        <dataType>String</dataType>
        <expression>SUBSTITUTE({!GetLabelLineDetail.Print_Quantity__c}, &quot;,&quot;, &quot;&quot;)</expression>
    </formulas>
    <formulas>
        <name>PrintQtyFormula1</name>
        <dataType>Number</dataType>
        <expression>{!GetLabelLineDetail.Print_Quantity_Num__c}</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
